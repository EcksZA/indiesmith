<%= form_for @store do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>

	<%= f.label :country %>
	<%= f.text_field :country %>

	<%= f.label "customer service email" %>
	<%= f.text_field :contact_email %>

	<%= f.label :phone %>
	<%= f.text_field :phone %>

	<%= f.label :email %>
	<%= f.text_field :email %>

	<%= f.label :password %>
	<%= f.text_field :password %>

	<%= f.submit %>
<% end %>